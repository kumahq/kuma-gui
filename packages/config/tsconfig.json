{
  "compilerOptions": {
    "esModuleInterop": true,
    "noErrorTruncation": true,
    "baseUrl": ".",
    "module": "preserve",
    "target": "esnext",
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "types": [
      "node",
      "cypress"
    ],
  },
  "include": [
    "**/*.ts"
  ]
}
