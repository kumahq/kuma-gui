{
  "name": "@kumahq/kuma-http-api",
  "version": "2.12.0",
  "private": true,
  "description": "Kuma OpenAPI to typescript converter, generated typescript types and mocks",
  "type": "module",
  "imports": {
    "#mocks": "./mocks/index.ts"
  },
  "exports": {
    ".": {
      "import": "./index.d.ts",
      "types": "./index.d.ts"
    },
    "./mocks": {
      "import": "./mocks/index.ts"
    },
    "./openapi.yaml": "./openapi.yaml"
  },
  "scripts": {
    "help": "make help"
  },
  "optionalDependencies": {
    "@faker-js/faker": "^10.1.0",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@kumahq/config": "*",
    "openapi-format": "^1.29.0",
    "openapi-typescript": "^7.10.1"
  }
}
