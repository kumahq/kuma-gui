{
  "name": "@kumahq/kuma-http-api",
  "version": "2.12.0",
  "private": true,
  "description": "Kuma OpenAPI to typescript converter, generated typescript types and mocks",
  "type": "module",
  "imports": {
    "#mocks": "./mocks/index.ts"
  },
  "exports": {
    ".": {
      "import": "./index.d.ts",
      "types": "./index.d.ts"
    },
    "./mocks": {
      "import": "./mocks/index.ts"
    }
  },
  "scripts": {
    "help": "make help"
  },
  "optionalDependencies": {
    "@faker-js/faker": "^10.0.0",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@kumahq/config": "*",
    "@redocly/cli": "^2.1.2",
    "openapi-overlays-js": "^0.2.0",
    "openapi-typescript": "^7.9.1"
  }
}
